<!DOCTYPE aesl-source>
<network>


<!--list of global events-->


<!--list of constants-->


<!--show keywords state-->
<keywords flag="true"/>


<!--node thymio-II-->
<node nodeId="22471" name="thymio-II">onevent prox
	if (prox.horizontal[0] &lt; 300) then
		System.out.print(prox.horizontal[0]);
		System.out.print(prox.horizontal[1]);
		System.out.print(prox.horizontal[2]);
		System.out.print(prox.horizontal[3]);
		System.out.print(prox.horizontal[4]);
		System.out.print(prox.horizontal[5]);
		System.out.print(prox.horizontal[6]);
end



#prox.ground.delta[0] : bottom left
#prox.ground.delta[1] : bottom right
#prox.horizontal[0] : front left
#prox.horizontal[1] : front middle-left
#prox.horizontal[2] : front middle
#prox.horizontal[3] : front middle-right
#prox.horizontal[4] : front right
#prox.horizontal[5] : back left
#prox.horizontal[6] : back right
</node>


</network>
